<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.owner.todo">
    <!-- 基础模块 END -->
    <application
        android:name="com.owner.baselibrary.common.AMSystemApp"
        android:allowBackup="true"
        android:icon="@mipmap/todo_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.owner.todo.view.activity.TasksActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.AddEditTaskActivity"/>
    </application>
</manifest>
